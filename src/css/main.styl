#page
  background-color: $backgroundColor2
  width: calc(100vw - 200px)
  height: calc(100vh - 60px)
  float: left
  padding: 0 $elementHSpacing
  overflow: auto
  position: relative

  & > *
    margin: $elementVSpacing 0
    display: block

  iframe
    width: 100%
    height: 100%
    border: 0
    padding: 0
    margin: 0

